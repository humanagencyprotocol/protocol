---
import { getEntry } from 'astro:content';
import ContentLayout from '../layouts/ContentLayout.astro';

const entry = await getEntry('docs', 'review');

// Only render if the review document exists
if (!entry) {
    return Astro.redirect('/protocol');
}

const { Content } = await entry.render();
---

<ContentLayout
    title={entry.data.title}
    version={entry.data.version}
    date={entry.data.date}
    prevPage={{ url: '/governance', label: 'Governance' }}
>
    <Content />
</ContentLayout>
